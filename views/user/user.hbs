<div class="row">
    <div class="col s12">
        <ul class="tabs">
            <li class="tab col s2"><a class="active" href="{{domainUrl}}/user" onclick="location.href = this.href">{{getLanguageString lang "MY_SUMMARY"}}</a></li>
            <li class="tab col s2"><a href="{{domainUrl}}/user/myDrafts" onclick="location.href = this.href">{{getLanguageString lang "MY_DRAFTS"}}</a></li>
            <li class="tab col s2"><a href="{{domainUrl}}/user/myPosts" onclick="location.href = this.href">{{getLanguageString lang "MY_POSTS"}}</a></li>
            <li class="tab col s2"><a href="{{domainUrl}}/user/myImages" onclick="location.href = this.href">{{getLanguageString lang "MY_IMAGES"}}</a></li>
            <li class="tab col s2"><a href="{{domainUrl}}/user/myBookmarks" onclick="location.href = this.href">{{getLanguageString lang "MY_BOOKMARKS"}}</a></li>
        </ul>
    </div>
</div>

<form ref='uploadForm'
      id='uploadForm'
      action='https://wearmenu.com/upload'
      method='post'
      encType="multipart/form-data">
    <input type="hidden" name="size" value="500" />
    <input type="file" name="sampleFile" />
    <input type='submit' value='Upload!' />
</form>


<form action="https://wearmenu.com/uploadUserImages" method="post" enctype="multipart/form-data">
    <input type="file" multiple="multiple" accept="image/*" name="uploadedImages"/><br/>
    <input type="submit" value="Upload" />
</form>

